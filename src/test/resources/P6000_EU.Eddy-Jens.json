{
  "P6000": {
    "sedPackage": "Sector Components/Pensions/P6000",
    "sedGVer": "$sedGVer",
    "sedVer": "$sedVer",
    "LocalCaseNumbers": {
      "LocalCaseNumber": [
        {
          "country": {
            "value": [
              "$nav.eessisak.land"
            ]
          },
          "caseNumber": "$nav.eessisak.saksnummer",
          "Institution": {
            "institutionID": "$nav.eessisak.institusjonsid",
            "institutionName": "$nav.eessisak.institusjonsnavn"
          }
        }
      ]
    },
    "InsuredPerson": {
      "PersonIdentification": {
        "familyName": "$nav.forsikretperson.person.etternavn",
        "forename": "$nav.forsikretperson.person.fornavn",
        "dateBirth": "$nav.forsikretperson.person.foedlselsdato",
        "sex": {
          "value": [
            "$nav.forsikretperson.person.kjoenn"
          ]
        },
        "familyNameAtBirth": "$nav.forsikretperson.person.etternavnvedfoedsel",
        "forenameAtBirth": "$nav.forsikretperson.person.forrnavnvedfoedsel",
        "PINPersonInEachInstitution": {
          "PersonalIdentificationNumber": [
            {
              "country": {
                "value": [
                  "$nav.forsikretperson.person.pin[x].land"
                ]
              },
              "personalIdentificationNumber": "$nav.forsikretperson.person.pin[x].identifikator",
              "sector": {
                "value": [
                  "$nav.forsikretperson.person.pin[x].sektor"
                ]
              },
              "Institution": {
                "institutionID": "$nav.forsikretperson.person.pin[x].institusjonsid",
                "institutionName": "$nav.forsikretperson.person.pin[x].institusjonsnavn"
              }
            }
          ]
        },
        "IfPINNotProvidedForEveryInstitutionPleaseProvide": {
          "PlaceBirth": {
            "town": "$nav.forsikretperson.person.foedested.by",
            "region": "$nav.forsikretperson.person.foedested.region",
            "country": {
              "value": [
                "$nav.forsikretperson.person.foedested.land"
              ]
            }
          },
          "fatherFamilyNameAtBirth": "$nav.forsikretperson.far.person.etternavnvedfoedsel",
          "forenameFather": "$nav.forsikretperson.far.person.fornavn",
          "motherFamilyNameAtBirth": "$nav.forsikretperson.mor.person.etternavnvedfoedsel",
          "forenameMother": "$nav.forsikretperson.mor.person.fornavn"
        }
      },
      "AdditionalInformationPerson": {
        "nationality": {
          "value": [
            "$nav.forsikretperson.person.statsborgerskap[x]"
          ]
        },
        "previousFamilyName": "$nav.forsikretperson.person.tidligereetternavn",
        "previousForename": "$nav.forsikretperson.person.tidligerefornavn"
      },
      "Address": {
        "street": "$nav.forsikretperson.adresse.gate",
        "buildingName": "$nav.forsikretperson.adresse.bygning",
        "town": "$nav.forsikretperson.adresse.by",
        "postalCode": "$nav.forsikretperson.adresse.postnummer",
        "region": "$nav.forsikretperson.adresse.region",
        "country": {
          "value": [
            "$nav.forsikretperson.adresse.land"
          ]
        }
      },
      "Decisions": {
        "Decision": [
          {
            "typePension": "$pensjon.sak.type",
            "definedPension": "$pensjon.vedtak.basertPaa",
            "PleaseFillInFollowingIfTypePensionOther": {
              "otherPension": "$pensjon.vedtak.basertPaaAnnen"
            },
            "decision": "$pensjon.vedtak.resultat",
            "awardBenefitArticle": "$pensjon.vedtak.beregning[x].artikkel",
            "startDatePensionRight": "$pensjon.vedtak.virkningsdato",
            "DetailsPensionPaymentPeriods": [
              {
                "payableFrom": "$pensjon.vedtak.beregning[x].periode.fom",
                "payableTo": "$pensjon.vedtak.beregning[x].periode.tom",
                "PensionAmount": {
                  "grossAmount": "$pensjon.vedtak.beregning[x].beloepBrutto.beloep",
                  "netAmount": "$pensjon.vedtak.beregning[x].beloepNetto.beloep",
                  "grossAmountBasicPension": "$pensjon.vedtak.beregning[x].beloepBrutto.ytelseskomponentGrunnpensjon",
                  "grossAmountSupplementaryPension": "$pensjon.vedtak.beregning[x].beloepBrutto.ytelseskomponentTilleggspensjon"
                },
                "currency": "$pensjon.vedtak.beregning[x].valuta",
                "paymentFrequency": "$pensjon.vedtak.beregning[x].utbetalingshyppighet",
                "PleaseFillInFollowingIfPaymentFrequencyOther": {
                  "otherPaymentFrequency": "$pensjon.vedtak.beregning[x].utbetalingshyppighetAnnen"
                }
              }
            ],
            "actualStartDatePensionPaymentsFromInstitution": "$pensjon.kjoeringsdato",
            "supplementsAdditionalFrequentPaymentsPension": "$pensjon.vedtak.beregning[x].beloepBrutto.ytelseskomponentAnnen",
            "basedVoluntaryContributions": "$pensjon.sak.vedtak.grunnlag.medlemskap",
            "basedOtherPersonInsurancePeriods": "$pensjon.sak.vedtak.beregning.grunnlag.opptjening.forsikredeAnnen",
            "creditedPeriodIndicator": "$pensjon.sak.vedtak.beregning.grunnlag.framtidigtrygdetid",
            "RejectionReasons": [
              {
                "rejectionReasons": "$pensjon.vedtak.avslag.begrunnelse[x]",
                "FillInIfRejectionReasonsOther": {
                  "otherReasons": "$pensjon.vedtak.avslag.begrunnelseAnnen"
                }
              }
            ],
            "PartialSuspensionPension": {
              "pensionPartlySuspendedIndicator": "$pensjon.vedtak.opphor.verdi",
              "PleaseFillInFollowingIfPensionPartlySuspendedYes": {
                "amount": "$pensjon.vedtak.opphor.utbetaling.beloepBrutto",
                "currency": "$pensjon.vedtak.opphor.utbetaling.valuta[x]",
                "reason": "$pensjon.vedtak.opphor.begrunnelse"
              }
            },
            "additionalInformation": "$pensjon.vedtak.begrunnelseAnnen"
          }
        ]
      },
      "Reductions": {
        "ReductionsPension": [
          {
            "pensionReducedPursuantOverlappingBenefitsSameKind": "$pensjon.vedtak.reduksjon[x].type",
            "pensionReducedPursuantOverlappingOrBenefitsOrIncome": "$pensjon.vedtak.reduksjon[x].arsak.inntekt",
            "FillInIfPensionHasBeenReduced": {
              "otherBenefitsSourceIncome": "$pensjon.vedtak.reduksjon.arsak.inntektAnnen"
            },
            "articleConcerningLimitationRuleOverlapping": "$pensjon.sak.vedtak.reduksjon.artikkeltype",
            "PeriodOverlapping": [
              {
                "startDateRuleAgainstOverlapping": "$pensjon.vedtak.virkningsdato",
                "endDateRuleAgainstOverlapping": "$ignore.endDateRuleAgainstOverlapping"
              }
            ]
          }
        ]
      },
      "AdditionalInformationDecision": {
        "benefitsAIndicator": "$pensjon.sak.artikkel44",
        "suspensionPension": "$pensjon.vedtak.opphor.dato",
        "withdrawalPension": "$pensjon.vedtak.opphor.annulleringdato",
        "dateWhenDecisionIssuedStatedDecision": "$pensjon.vedtak.dato",
        "InformationRightAReview": {
          "timeLimitsForReview": "$pensjon.sak.kravtype[x].datoFrist",
          "WhereAddressReview": [
            {
              "Institution": {
                "institutionID": "$nav.eessisak.institusjonsid",
                "institutionName": "$nav.eessisak.institusjonsnavn"
              },
              "Address": {
                "street": "$nav.eessisak.institusjonsadresse",
                "buildingName": "$ignore.buildingName",
                "town": "$nav.eessisak.institusjonsadresse.poststed",
                "postalCode": "$nav.eessisak.institusjonsadresse.postnummer",
                "region": "$ignore.region",
                "country": "$nav.eessisak.institusjonsadresse.land"
              }
            }
          ]
        },
        "decisionGivenAResultReviewAccordingArt4829872009Indicator": "$pensjon.vedtak.artikkel48",
        "InstitutionGivingDecisionIfDifferentFromSending": {
          "Institution": {
            "country": "$nav.eessisak.institusjonsadresse.land",
            "personalIdentificationNumber": "$nav.forsikretperson.person.pin[x].identifikator",
            "sector": "$nav.forsikretperson.person.pin[x].sektor",
            "Institution": {
              "institutionID": "$nav.eessisak.institusjonsID",
              "institutionName": "$nav.eessisak.institusjonsnavn"
            }
          },
          "caseNumber": "$nav.eessisak.saksnummer"
        },
        "referenceNumberDecisionIfOtherThanCaseNumber": "$nav.eessisak.saksnummerAnnen",
        "otherInformation": "$ignore.otherInformation"
      }
    }
  }
}